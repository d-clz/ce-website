uched = true;\n        }\n        this.$emit('input', check);\n      }\n    },\n    inlineClass() {\n      if (this.inline) {\n        return `form-check-inline`;\n      }\n\n      return ``;\n    }\n  },\n  created() {\n    this.cbId = Math.random()\n      .toString(16)\n      .slice(2);\n  }\n};\n</script>\n"]}]}