AAA,KAAA,OAAA;AACA;;AACA,gBAAA,QAAA,CAAA,MAAA,CAAA,UAAA,EAAA,KAAA,OAAA,CAAA,QAAA;;AAEA,oBAAA,KAAA,QAAA,EAAA,EAAA;AACA,kBAAA,cAAA,CACA,iBADA,CACA,QADA,EACA,KAAA,MAAA,CAAA,MAAA,CAAA,EADA,EAEA,IAFA,CAEA,UAAA,MAAA,EAAA;AACA,wBAAA,MAAA,IAAA,MAAA,CAAA,OAAA,EAAA;AACA,sBAAA,KAAA,CAAA,OAAA,CAAA;AACA,wBAAA,aAAA,EAAA,QADA;AAEA,wBAAA,eAAA,EAAA,QAFA;AAGA,wBAAA,IAAA,EAAA,SAHA;AAIA,wBAAA,OAAA,EAAA;AAJA,uBAAA;;AAMA,sBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,wBAAA,IAAA,EAAA;AAAA,uBAAA;AACA;AACA,mBAZA,EAaA,KAbA,CAaA,UAAA,GAAA,EAAA;AACA,oBAAA,KAAA,CAAA,OAAA,CAAA;AACA,sBAAA,aAAA,EAAA,QADA;AAEA,sBAAA,eAAA,EAAA,QAFA;AAGA,sBAAA,IAAA,EAAA,QAHA;AAIA,sBAAA,OAAA,EAAA,GAAA,CAAA;AAJA,qBAAA;AAMA,mBApBA;AAqBA;;AA5CA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AA8CA,IAAA,QA9CA,sBA8CA;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,UAAA,CAAA,KAAA,OAAA,IAAA,KAAA,OAAA,CAAA,QAAA,GAAA,CAAA,EAAA;AACA,aAAA,OAAA,CAAA;AACA,UAAA,aAAA,EAAA,QADA;AAEA,UAAA,eAAA,EAAA,QAFA;AAGA,UAAA,IAAA,EAAA,QAHA;AAIA,UAAA,OAAA,EAAA;AAJA,SAAA;AAMA,QAAA,KAAA,GAAA,KAAA;AACA;;AAEA,aAAA,KAAA;AACA,KA3DA;AA6DA,IAAA,WA7DA,uBA6DA,IA7DA,EA6DA;AACA,WAAA,OAAA,CAAA,IAAA,GAAA,GAAA,CAAA,eAAA,CAAA,IAAA,CAAA;AACA,WAAA,KAAA,CAAA,WAAA,EAAA,IAAA;AACA;AAhEA,GAbA;AA+EA,EAAA,OA/EA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgFA,mBAAA,IAAA;;AAhFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,CAAA","sourcesContent":["<template>\n  <div>\n    <base-header class=\"pb-6 pb-8 pt-5 pt-md-8 bg-gradient-success\">\n      <!-- Card stats -->\n    </base-header>\n\n    <b-container fluid class=\"mt--6\">\n      <b-row>\n        <!-- Job Detail -->\n        <b-col xl=\"12\">\n          <card>\n            <b-row align-v=\"center\" slot=\"header\">\n              <b-col>\n                <h3 class=\"mb-0\">Update Gallery</h3>\n              </b-col>\n            </b-row>\n            <b-row>\n              <b-col lg=\"8\"\n                ><b-form @submit.prevent=\"onSubmit\">\n                  <div class=\"pl-lg-4\">\n                    <b-row>\n                      <b-col>\n                        <base-input\n                          type=\"number\"\n                          label=\"Position\"\n                          placeholder=\"Position\"\n                          v-model=\"gallery.position\"\n                          name=\"Position\"\n                          ref=\"Position\"\n                          :rules=\"{ required: true }\"\n                        >\n                        </base-input>\n                      </b-col>\n                    </b-row>\n                  </div>\n                  <base-button\n                    type=\"primary\"\n                    :disabled=\"submitting\"\n                    :loading=\"submitting\"\n                    native-type=\"submit\"\n                    class=\"my-4\"\n                  >\n                    Update\n                  </base-button>\n                </b-form></b-col\n              >\n              <b-col lg=\"4\">\n                <img :src=\"gallery.path\" width=\"100%\" />\n                <label class=\"form-control-label\">Image</label>\n\n                <b-form-file\n                  @input=\"fileChanges\"\n                  v-model=\"imgFile\"\n                  placeholder=\"Select file\"\n                  drop-placeholder=\"Drop file here...\"\n                  accept=\"image/jpeg, image/png\"\n                  class=\"button\"\n                ></b-form-file>\n              </b-col>\n            </b-row>\n          </card>\n        </b-col>\n      </b-row>\n    </b-container>\n  </div>\n</template>\n<script>\nimport galleryService from '@/api/galleryService'\n\nexport default {\n  data() {\n    return {\n      gallery: {\n        path: '',\n        position: '',\n        filename: '',\n      },\n      imgFile: null,\n      submitting: false,\n      isLoading: false,\n    }\n  },\n  methods: {\n    async init() {\n      this.isLoading = true\n      const id = this.$route.params.id\n      //   await galleryService.getImageById(id).then((data) => {\n      //     console.log('image', data)\n      //   })\n      let res = await galleryService.getGalleryById(id)\n      console.log('res', res)\n      if (res && res.success) {\n        this.gallery = res.gallery\n        this.gallery.path = 'http://localhost:3000/public/send-image/' + this.gallery.filename\n      }\n    },\n    async onSubmit() {\n      let formData = new FormData()\n      formData.append('vivexelt_pic', null)\n      if (this.imgFile != null) {\n        formData.append('vivexelt_pic', this.imgFile)\n      }\n      formData.append('position', this.gallery.position)\n\n      if (this.validate()) {\n        galleryService\n          .updateGalleryById(formData, this.$route.params.id)\n          .then((result) => {\n            if (result && result.success) {\n              this.$notify({\n                verticalAlign: 'bottom',\n                horizontalAlign: 'center',\n                type: 'success',\n                message: 'Update success',\n              })\n              this.$router.push({ name: 'gallerys' })\n            }\n          })\n          .catch((err) => {\n            this.$notify({\n              verticalAlign: 'bottom',\n              horizontalAlign: 'center',\n              type: 'danger',\n              message: err.message,\n            })\n          })\n      }\n    },\n    validate() {\n      let valid = true\n      if (!this.gallery && this.gallery.position < 1) {\n        this.$notify({\n          verticalAlign: 'bottom',\n          horizontalAlign: 'center',\n          type: 'danger',\n          message: 'Title is invalid',\n        })\n        valid = false\n      }\n\n      return valid\n    },\n\n    fileChanges(file) {\n      this.gallery.path = URL.createObjectURL(file)\n      this.$emit('imgChange', file)\n    },\n  },\n  async mounted() {\n    this.init()\n  },\n}\n</script>\n"],"sourceRoot":"src/views/Gallerys"}]}